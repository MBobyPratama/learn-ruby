<div class="main post-index">
  <table border=1>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Email</th>
      <th>Action</th>
    </tr>
    <% no=0 
    @users.each do |value| %>
    <tr>
      <td><%= no= no+1 %></td>
      <td><%= value.name %></td>
      <td><%= value.email %></td>
      <td>
        <a href="/crud/detail<%= value.id %>">Detail</a>
        <a href="">Edit</a>
        <a href="">Delete</a>
      </td>
    </tr>
    <%end%>
  </table>
</div>